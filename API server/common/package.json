{
  "name": "@xrengine/common",
  "version": "0.4.6",
  "main": "lib/common.umd.js",
  "module": "lib/common.es.js",
  "types": "lib/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "./src/scenes-templates": "./lib/scenes-templates/index.js",
    "./src/*": "./lib/*.js",
    "./src/**/": "./lib/**/index.js",
    "./src/**/*": "./lib/**/*.js"
  },
  "description": "Shared types for other XREngine packages",
  "scripts": {
    "check-errors": "tsc --noemit",
    "build-publish": "rimraf lib && cross-env NODE_OPTIONS=--max_old_space_size=4096 NODE_ENV=production node build.js",
    "update-name-gh-package": "node scripts/update-name-gh-package.js"
  },
  "repository": "http://github.com/XRFoundation/XREngine",
  "author": "XREngine",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-alias": "3.1.5",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.0.4",
    "@rollup/plugin-replace": "3.0.0",
    "create-ts-index": "1.13.6",
    "cross-env": "7.0.3",
    "lodash.camelcase": "4.3.0",
    "rimraf": "3.0.2",
    "rollup": "2.56.3",
    "rollup-plugin-livereload": "2.0.5",
    "rollup-plugin-node-polyfills": "0.2.1",
    "rollup-plugin-sass": "1.2.8",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.30.0",
    "tiny-glob": "0.2.9"
  },
  "gitHead": "2313453697ca7c6b8d36b3b166b5a6445fe1c851",
  "dependencies": {
    "react-lottie": "^1.2.3"
